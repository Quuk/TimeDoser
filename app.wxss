@import "colorui/main.wxss";
@import "colorui/icon.wxss";

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.pageBox {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.pageHead {
    margin-top: 20%;
}

.header {
    height: 50px;
    width: 50px;
    border: 1px solid red;
    border-radius: 25px;
}
