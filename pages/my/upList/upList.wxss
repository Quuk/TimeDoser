.page-wait {
    margin-top: 30%;
    width: 100%;
    font-size: 15px;
}

.version-content {
    position: relative;
    width: 100%;
}

.version-avatar {
    padding-right: 16rpx;
}

.version-remark-logo {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-right: 20rpx;
    width: 140rpx;

}

.version-remark {
    display: flex;
    flex-direction: row;
}

.version-input{
    width: 100%;
    border: 1px #a38f72 solid;
    border-radius: 100rpx;
    padding: 0 10px 0 10px;
    font-size: 13px;
    color: #868686;
}

.temp{
    border: red 1px solid;
}
