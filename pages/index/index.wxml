<view class="pageBox">
    <view class="pageHead"/>
    <view id="time">
        <text class="cuIcon-time"></text>
        <text>{{minute}}分{{second}}秒</text>
        <text class="cuIcon-time"></text>
    </view>
</view>
<view class="padding flex flex-direction">
    <button class="cu-btn bg-red margin-tb-sm lg" bindtap="setTime">开始</button>
    <button class="cu-btn bg-grey lg" bindtap="toSetting">设置</button>
</view>